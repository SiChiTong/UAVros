      <visual name='cgo3_camera_visual'>
        <pose>0.03 0 0.07 0 0 3.141592</pose>
        <geometry>
          <mesh>
            <scale>0.001 0.001 0.001</scale>
            <uri>model://solo_gimbal/meshes/cgo3_camera_remeshed_v1.stl</uri>
