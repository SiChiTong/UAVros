            <uri>file://media/materials/scripts/gazebo.material</uri>
          </script>
        </material>
      </visual>
      <sensor name="camera_imu" type="imu">
        <always_on>1</always_on>
      </sensor>
      <sensor name="camera" type="camera">
        <pose>0.08 0 -0.092 0 0 0</pose>
        <camera>
          <horizontal_fov>2.18</horizontal_fov>
          <image>
            <format>R8G8B8</format>
            <width>1280</width>
            <height>720</height>
          </image>
          <clip>
            <near>0.05</near>
            <far>15000</far>
          </clip>
        </camera>
        <always_on>1</always_on>
        <update_rate>30</update_rate>
        <visualize>true</visualize>
        <!-- gazebo_ros_plugin added by spx -->
        <plugin name='camera_ros_plugin' filename='libgazebo_ros_camera.so'>
          <robotNamespace></robotNamespace> <!--default is the robot model name-->
          <alwaysOn>true</alwaysOn>
          <imageTopicName>gimbal_image</imageTopicName>
          <cameraInfoTopicName>camera_info</cameraInfoTopicName>
          <updateRate>30.0</updateRate>
          <cameraName>amov_gimbal_ros</cameraName>
          <frameName>/robot_camera_link</frameName>
          <CxPrime>640</CxPrime>
          <Cx>640</Cx>
          <Cy>360</Cy>
          <hackBaseline>0</hackBaseline>
          <focalLength>333.838991</focalLength> <!-- focal_length = image_width / (2.0 * tan(horizontal_fov/2.0)) -->
          <distortionK1>0.0</distortionK1>
          <distortionK2>0.0</distortionK2>
          <distortionK3>0.0</distortionK3>
          <distortionT1>0.0</distortionT1>
          <distortionT2>0.0</distortionT2>
        </plugin>    
        <!-- Two GStreamer camera plugins (need a lot of CPU! Consider lowering the camera image size). Or you can change the <useCuda> added by spx to true to use GPU for acceleration. Refer to https://github.com/PX4/PX4-SITL_gazebo-classic/tree/main -->
        <!--plugin name="GstCameraPlugin" filename="libgazebo_gst_camera_plugin.so">
            <robotNamespace></robotNamespace>
            <useCuda>false</useCuda>
            <udpHost>127.0.0.1</udpHost>
            <udpPort>5600</udpPort>
        </plugin-->
        <!--plugin name="CameraManagerPlugin" filename="libgazebo_camera_manager_plugin.so">
